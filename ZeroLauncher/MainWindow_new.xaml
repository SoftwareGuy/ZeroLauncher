        xmlns:local="clr-namespace:ZeroLauncher"
        mc:Ignorable="d"
        Title="ZeroLauncher Setup Wizard" Height="400" Width="400">
    <Grid>
        <ContentControl x:Name="setupWizard" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Height="317" Width="392"/>
        <Button x:Name="buttonNext" Content="Next" HorizontalAlignment="Left" Margin="294,322,0,0" VerticalAlignment="Top" Width="75" Click="buttonNext_Click"/>
        <Button x:Name="buttonPrev" Content="Previous" HorizontalAlignment="Left" Margin="214,322,0,0" VerticalAlignment="Top" Width="75" Click="buttonPrev_Click" IsEnabled="False"/>

